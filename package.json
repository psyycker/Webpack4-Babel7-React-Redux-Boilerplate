{
  "version": "2.0.0",
  "author": {
    "name": "Remy Villulles"
  },
  "description": "React boilerplate with ES7, Express.js, Webpack and airbnb linter",
  "license": "MIT",
  "engines": {
    "node": ">8.11.1"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon  --watch src --exec babel-node src/server --source-maps",
    "start": "cross-env NODE_ENV=production babel-node build/server",
    "prebuild": "rimraf build",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --config=config/webpack.config.prod.js",
    "build:server": "copyfiles -a -u 1 src/server/**/**/* build",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "postcss": {},
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.0",
    "@babel/plugin-proposal-decorators": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.2.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-watch": "2.0.7",
    "copyfiles": "2.1.0",
    "css-loader": "1.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-fbjs": "2.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-relay": "1.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "0.5.5",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "immutable": "4.0.0-rc.12",
    "intl": "1.2.5",
    "jest": "23.6.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-enzyme": "7.0.1",
    "jest-fetch-mock": "^2.0.1",
    "node-sass": "4.10.0",
    "nodemon": "^1.18.7",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.4.0",
    "prettier": "1.15.3",
    "react-hot-loader": "4.3.12",
    "react-intl": "2.7.2",
    "rimraf": "2.6.2",
    "sanitize.css": "8.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.4",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "express": "4.16.4",
    "fast-safe-stringify": "2.0.6",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "typescript": "^3.2.1",
    "winston": "3.1.0"
  }
}
