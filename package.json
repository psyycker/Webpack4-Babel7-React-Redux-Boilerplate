{
  "version": "2.0.0",
  "author": {
    "name": "Remy Villulles"
  },
  "description": "React boilerplate with ES7, Express.js, Webpack and airbnb linter",
  "license": "MIT",
  "engines": {
    "node": ">=12.16.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development cross-env HTTP_PORT=3214 nodemon  --watch src --exec babel-node src/server --source-maps",
    "start": "cross-env NODE_ENV=production cross-env HTTP_PORT=3214 babel-node build/server",
    "prebuild": "rimraf build",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --config=config/webpack.config.prod.js",
    "build:server": "copyfiles -a -u 1 src/server/**/**/* build",
    "test": "jest",
    "install:clean": "rimraf package-lock.json node_modules && npm install"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "postcss": {},
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "7.10.1",
    "@babel/plugin-proposal-function-sent": "7.10.1",
    "@babel/plugin-proposal-json-strings": "7.10.1",
    "@babel/plugin-proposal-numeric-separator": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.1",
    "@babel/runtime": "7.10.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "8.1.0",
    "babel-watch": "7.0.0",
    "copyfiles": "2.3.0",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-fbjs": "3.1.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-relay": "1.7.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "1.1.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "4.3.0",
    "immutable": "4.0.0-rc.12",
    "intl": "1.2.5",
    "jest": "26.0.1",
    "jest-css-modules-transform": "^4.0.0",
    "jest-enzyme": "7.1.2",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "4.14.1",
    "nodemon": "^2.0.4",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "prettier-eslint": "^10.1.1",
    "react-hot-loader": "4.12.21",
    "react-intl": "4.6.9",
    "rimraf": "^3.0.2",
    "sanitize.css": "11.0.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.2",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "fast-safe-stringify": "2.0.7",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "react-router-redux": "4.0.8",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "winston": "3.2.1"
  }
}
