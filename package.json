{
  "version": "3.0.0",
  "author": {
    "name": "Remy Villulles"
  },
  "description": "React boilerplate with ES7, Express.js, Webpack and airbnb linter",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development cross-env HTTP_PORT=3214 nodemon  --watch src --exec babel-node src/server --source-maps",
    "start": "cross-env NODE_ENV=production cross-env HTTP_PORT=3214 babel-node build/server",
    "prebuild": "rimraf build",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --progress --config=config/webpack.config.prod.js",
    "build:server": "copyfiles -a -u 1 src/server/**/**/* build",
    "test": "jest",
    "install:clean": "rimraf package-lock.json node_modules && npm install",
    "lint": "eslint src --ext .jsx,.js --fix"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "postcss": {},
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.12",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-function-sent": "7.12.1",
    "@babel/plugin-proposal-json-strings": "7.12.1",
    "@babel/plugin-proposal-numeric-separator": "7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-throw-expressions": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/register": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.2.2",
    "babel-watch": "7.0.0",
    "copyfiles": "2.4.1",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-fbjs": "3.1.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-relay": "1.8.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "1.3.2",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.6.3",
    "jest-css-modules-transform": "^4.1.0",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^1.3.3",
    "nodemon": "^2.0.6",
    "postcss-loader": "4.1.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "prettier-eslint": "^12.0.0",
    "react-hot-loader": "4.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "5.11.0",
    "webpack-cli": "4.3.0",
    "webpack-dev-middleware": "4.0.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  }
}
